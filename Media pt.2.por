programa {
  funcao inicio() {
    real un1,un2,un3,un4,un5,media

    escreva("Insira sua primeira nota: ")
    leia(un1)

    escreva("Insira sua segunda nota: ")
    leia(un2)

    escreva("Insira sua terceira nota: ")
    leia(un3)

    escreva("Insira sua quarta nota: ")
    leia(un4)

    escreva("Insira sua quinta nota: ")
    leia(un5)

    media=(un1+un2+un3+un4+un5)/5

    escreva("Seua média é :",media)
  }
}
